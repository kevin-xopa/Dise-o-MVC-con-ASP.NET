<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/css/materialize.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body class="cyan darken-2 white-text">
    <nav>
        <div class="nav-wrapper black accent-2">
            <a href="#" class="brand-logo">Hello!</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li><a href="#">Inicio</a></li>
            </ul>
        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year -Hello!  <i class="material-icons">thumb_up</i></p>
        </footer>
    </div>

    <script src="~/Content/js/jquery-3.5.1.min.js"></script>
    <script src="~/Content/js/materialize.js"></script>
</body>
</html>